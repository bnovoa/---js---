cdkeyIput.on('focus',function(){
	if(cdkeyIput.val() == "请填写手机号码"){
		cdkeyIput.attr("value","");
	}else{
		cdkeyIput.attr("value",$(this).val());
	}
});

cdkeyIput.on('blur',function(){
	if(cdkeyIput.val() == ""){
		cdkeyIput.attr("value","请填写手机号码");
	}else{
		cdkeyIput.attr("value",$(this).val());
	}
});

