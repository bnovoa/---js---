{{#each data}}
<div class="cartBarClum list-item" >
	 <input class="check check-field"  type="checkbox" name="check-field" action-pid="{{productId}}" action-type="{{amount}}"  checked="checked" value=""/>
	 <div class="imgBox"><img src="{{imageUrl}}" width="70" height="70" /></div>
	 <span class="span01">乐跑手环</span>
	 <span class="span02">{{color}}</span>
	 <span class="span03">￥<span class="item-price">{{price}}</span></span>
	 <span class="span04">有货</span>
	 <div class="span05 quantity-container">
	 	<span class="icon_Minus"></span>
	 	<input class="Quantity quantity-field" value="{{amount}}" type="text" name="textfield" id="textfield" />
	 	<span class="icon_Plus"></span>
	 </div>
	 <a href="#" class="span06 delete">删除</a>
</div>
{{/each}}
